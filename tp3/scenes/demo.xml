<SXG>
    <scene root="demoRoot" axis_length="5" /> <!-- deve definir-se um objeto para raiz da arvore, assim -->
    <!-- como o comprimento dos tres eixos (cilindros) -->

    <!-- ___________________________________VIEWS______________________________________ -->
    <views default="menu">
        <!-- at least one of the following type of cameras must be declared -->

        <!-- Vista Geral - default -->
        <perspective id="menu" near="0.1" far="500" angle="57">
            <from x="0" y="15" z="-0.3" />
            <to x="-0.3" y="0" z="0" />
        </perspective>


        <!-- Vista de cima -->
        <perspective id="trackView" near="0.1" far="500" angle="90">
            <from x="256" y="250" z="230" />
            <to x="256" y="0" z="256" />
        </perspective>

        <perspective id="demoCam1" near="0.1" far="500" angle="90">
            <from x="51.1" y="2" z="211.5" />
            <to x="16.98" y="0" z="10.01" />
        </perspective>

        <perspective id="demoCam2" near="0.1" far="500" angle="90">
            <from x="51.1" y="20" z="120.5" />
            <to x="200.1" y="0" z="311.5" />
        </perspective>
        <perspective id="demoCam3" near="0.1" far="500" angle="90">
            <from x="51.1" y="60" z="211.5" />
            <to x="200.1" y="0" z="311.5" />
        </perspective>
    </views>

    <ambient>
        <ambient r="0.1" g="0.1" b="0.1" a="1.0" /> <!-- global ambient -->
        <background r="0.694" g="0.850" b="0.930" a="1.0" /> <!-- background color -->
    </ambient>
    <!-- ___________________________________LIGHTS______________________________________ -->
    <lights>
        <!-- this block "light" must be repeated as necessary with different "id". At least one light should be present, and a maximum of eight lights is accepted. -->
        <omni id="RightFarLight" enabled="1">
            <!-- light identifier; enable/disable -->
            <location x="50" y="50" z="450" w="1.0" /> <!-- light location; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" /> <!-- ambient component -->
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" /> <!-- diffuse component -->
            <specular r="1" g="1" b="1" a="1.0" /> <!-- specular component -->
        </omni>

        <omni id="RightNearLight" enabled="1">
            <location x="50" y="50" z="50" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </omni>
        <omni id="menuLight" enabled="1">
            <location x="2" y="100" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </omni>

        <omni id="LeftNearLight" enabled="1">
            <location x="450" y="50" z="50" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </omni>

        <omni id="TrackLight" enabled="1">
            <location x="460" y="50" z="55" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </omni>

        <omni id="LeftFarLight" enabled="1">
            <location x="450" y="50" z="450" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </omni>

        <omni id="CenteredLight" enabled="1">
            <location x="256" y="50" z="256" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
        <!-- <spot id="spotLight" enabled="1" angle="90" exponent="50">

            <location x="0" y="20" z="0" w="0.0" />
            <target x="-0.5" y="3" z="12" />
            <ambient r="0.5" g="0.5" b="0.5" a="1" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </spot> -->

    </lights>
    <!-- ___________________________________TEXTURES______________________________________ -->
    <textures>
        <!-- this block "TEXTURE" must be repeated as necessary with different "id" -->
        <texture id="trackBackground" file="./scenes/images/trackbackground.png" />

        <texture id="b2rectTex" file="./scenes/images/fwcSponsor.png" />
        <texture id="surfaceRTex" file="./scenes/images/surfaceR.png" />
        <texture id="surfaceLTex" file="./scenes/images/surfaceL.png" />
        <texture id="frontwingTex" file="./scenes/images/frontwingSponsor.jpeg" />
        <texture id="rearwingFrontTex" file="./scenes/images/rwFront.png" />
        <texture id="rearwingBackTex" file="./scenes/images/rwBack.png" />
        <texture id="redTex" file="./scenes/images/red.png" />
        <texture id="sideWingTex" file="./scenes/images/santanderSponsor.png" />
        <texture id="wingTex" file="./scenes/images/wingSponsor.png" />
        <texture id="floorTex" file="./scenes/images/carbonFiber.jpeg" />
        <texture id="helmetTex" file="./scenes/images/helmet.png" />

    </textures>
    <!-- ___________________________________MATERIALS______________________________________ -->
    <materials>
        <!-- the "MATERIAL" block may be repeated as required. Each defined material
		 requires a distinct "id". At least one material should be present. -->

        <material id="material" shininess="100">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" /> <!-- emissive component -->
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" /> <!-- ambient reflection -->
            <diffuse r="1" g="1.0" b="1.0" a="1.0" /> <!-- diffuse reflection -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" /> <!-- specular reflection -->
        </material>

        <material id="redMaterial" shininess="10">
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0.199" g="0.019" b="0.043" a="1.0" />
            <diffuse r="0.199" g="0.019" b="0.043" a="1.0" />
            <specular r="0.228" g="0.207" b="0.178" a="1.0" />
        </material>

        <material id="whiteMaterial" shininess="100">
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="0.255" g="0.255" b="0.255" a="1.0" />
            <specular r="0.7" g="0.7" b="0.7" a="1.0" />
        </material>

        <material id="floorMaterial" shininess="50">
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0.228" g="0.207" b="0.178" a="1.0" />
            <diffuse r="0.228" g="0.207" b="0.178" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </material>
    </materials>

        <!-- the "primitives" block may be repeated as required. Each defined primitive
		requires a distinct "id". At least one material should be present. -->
        <primitive id="Rectangle">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>

        <primitive id="Triangle">
            <triangle x1="0" y1="0" z1="0" x2="1" y2="0" z2="0" x3="0.5" y3="1.1" z3="0" />
        </primitive>

        <primitive id="Sphere">
            <sphere radius="1.0" slices="30" stacks="30" />
        </primitive>

        <primitive id="Plane">
            <plane npartsU="100" npartsV="100" />
        </primitive>


    <!-- ___________________________________COMPONENTS______________________________________ -->
    <components>
        <!-- Uma "component" é um objeto composto e pode ser -->
        <!-- usada em nos intermédios -->
        <!-- bloco "transformation" e' obrigatorio -->


        <!-- CHÃO -->
        <component id="track">
            <transformation>
                <transformationref id="court" />
            </transformation>
            <materials>
                <material id="floorMaterial" />
            </materials>
            <texture id="trackBackground" length_s="1" length_t="2" />
            <children>
                <primitiveref id="Plane" />
            </children>
        </component>

        <component id="leftSurface">
            <transformation>
                <transformationref id="leftSurface" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="surface" />
            </children>
